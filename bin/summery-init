#!/usr/bin/env node

const chalk = require('chalk');
const inquirer = require('inquirer');
const generator = require('../lib/index');
console.log(12312);
inquirer.prompt([
  {
    type: 'input',
    message: '项目名称？',
    default: 'summery-project',
    name: 'projectName'
  }])
  .then( (answers)=> {
    generator(answers);
  })
